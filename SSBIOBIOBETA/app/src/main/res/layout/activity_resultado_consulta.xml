<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background_dos"
    tools:context=".ResultadoConsulta">
    <ImageView
        android:id="@+id/imageResultadoHora"
        android:layout_width="263dp"
        android:layout_height="182dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="24dp"
        app:srcCompat="@drawable/consulta_hora" />


    <TextView
        android:id="@+id/resultado_descripcionCodigo"
        android:layout_width="match_parent"
        android:layout_height="58dp"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/imageResultadoHora"
        android:text="DescripciÃ³n:"
        android:textColor="#0f69b4"
        android:textSize="20sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/resultado_paciente"
        android:layout_width="match_parent"
        android:layout_height="58dp"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/resultado_descripcionCodigo"

        android:textColor="#0f69b4"
        android:textSize="20sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/resultado_fechaAsiganda"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/resultado_paciente"

        android:textStyle="bold"
        android:textColor="#0f69b4"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/resultado_profesional"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/resultado_fechaAsiganda"

        android:textStyle="bold"
        android:textColor="#0f69b4"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/resultado_policlinico"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/resultado_profesional"

        android:textStyle="bold"
        android:textColor="#0f69b4"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/resultado_ubicacion"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/resultado_policlinico"

        android:textStyle="bold"
        android:textColor="#0f69b4"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/resultado_tipoHora"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/resultado_ubicacion"

        android:textStyle="bold"
        android:textColor="#0f69b4"
        android:textSize="20sp" />


</RelativeLayout>